@model IEnumerable<WebApplication.ViewModels.CoachViewModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Archived Coaches</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CoachId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoachMake)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoachModel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegistrationPlate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CoachCapacity)
        </th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CoachId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoachMake)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoachModel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationPlate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoachCapacity)
        </td>
    </tr>
}

</table>

